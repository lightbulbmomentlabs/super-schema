{
  "name": "aeo-schema-generator-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc || true && npm run copy-client",
    "copy-client": "mkdir -p dist/client && cp -r ../client/dist/* dist/client/",
    "postinstall": "PUPPETEER_SKIP_DOWNLOAD=false npx puppeteer browsers install chrome --path=/workspace/.cache/puppeteer || npx @puppeteer/browsers install chrome@stable --path=/workspace/.cache/puppeteer || true",
    "start": "node dist/src/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@clerk/express": "^0.1.1",
    "@hubspot/api-client": "^13.4.0",
    "@supabase/supabase-js": "^2.45.4",
    "@types/cheerio": "^0.22.35",
    "aeo-schema-generator-shared": "*",
    "axios": "^1.7.7",
    "bullmq": "^5.12.14",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "fast-xml-parser": "^5.2.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "openai": "^4.62.1",
    "puppeteer": "^23.4.1",
    "rate-limiter-flexible": "^5.0.3",
    "redis": "^4.7.0",
    "robots-parser": "^3.0.1",
    "stripe": "^16.12.0",
    "svix": "^1.28.0",
    "validator": "^13.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.0",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^8.57.0",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4"
  }
}
